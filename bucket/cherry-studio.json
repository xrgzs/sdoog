{
    "version": "1.0.5",
    "description": "üçí Cherry Studio is a desktop client that supports for multiple LLM providers.",
    "homepage": "https://cherry-ai.com/",
    "license": {
        "identifier": "Apache-2.0",
        "url": "https://github.com/CherryHQ/cherry-studio/blob/main/LICENSE"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/CherryHQ/cherry-studio/releases/download/v1.0.5/Cherry-Studio-1.0.5-setup.exe#/dl.7z",
            "hash": "sha512:dc418a512194477d741d9435e5357edbd7336096a1589abd201453db856cbe107898cd156446e0cb90d78f8939655702d885e90deaa6aa467ccc77659a6af1d7",
            "pre_install": "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\""
        }
    },
    "shortcuts": [
        [
            "Cherry Studio.exe",
            "Cherry Studio"
        ]
    ],
    "installer": {
        "script": [
            "Remove-Item \"$dir\\`$*\" -Force -Recurse",
            ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
            "New-PersistDirectory \"$env:APPDATA\\CherryStudio\" \"$persist_dir\\AppData\" -Migrate"
        ]
    },
    "pre_uninstall": [
        "$bucket = $install.bucket",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "Stop-App",
        "Remove-Junction \"$env:APPDATA\\CherryStudio\""
    ],
    "checkver": {
        "github": "https://github.com/CherryHQ/cherry-studio"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/CherryHQ/cherry-studio/releases/download/v$version/Cherry-Studio-$version-setup.exe#/dl.7z",
                "hash": {
                    "mode": "extract",
                    "url": "$baseurl/latest.yml",
                    "regex": "sha512:\\s+$base64"
                }
            }
        }
    }
}
